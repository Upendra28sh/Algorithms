import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) 
    {
        int count=1;
        Scanner sc=new Scanner(System.in);
        for(int i=1;sc.hasNext()==true;i++)
        {
            String a=sc.nextLine();
            System.out.println(i+" "+a);
        }
    }
}
